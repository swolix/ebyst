ACTION TEST = DO_TEST;

PROCEDURE DO_TEST;
    FOR i = 0 TO 3;
        EXPORT "TEST", i;
    NEXT i;
    FOR i = 0 TO 4 STEP 2;
        EXPORT "TEST", i + 10;
    NEXT i;
    INTEGER a = 0;
    INTEGER b = 10;
    FOR i = a TO b;
        EXPORT "TEST", i + 20;
        i = i + 2;
    NEXT i;
    FOR i = 10 TO 5 step -2;
        EXPORT "TEST", i + 30;
    NEXT i;

    FOR i = 0 to 2;
        FOR j = 0 to 2;
            EXPORT "TEST", i+j;
        NEXT j;
    NEXT i;
ENDPROC;

CRC 0;
