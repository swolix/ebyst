NOTE "TEST" "1";
NOTE "TEST" "3";
NOTE "TEST" "11";
NOTE "TEST" "13";
NOTE "TEST" "21";
NOTE "TEST" "23";

ACTION TEST = DO_TEST, DO_TEST2, DO_TEST3;

PROCEDURE DO_TEST2;
    EXPORT "TEST", 11;
    GOTO L1;
    EXPORT "TEST", 12;
L1:
    EXPORT "TEST", 13;
ENDPROC;

PROCEDURE DO_TEST;
    EXPORT "TEST", 1;
    GOTO L1;
    EXPORT "TEST", 2;
L1:
    EXPORT "TEST", 3;
ENDPROC;

PROCEDURE DO_TEST3;
    INTEGER a = 3;
    EXPORT "TEST", 21;
    IF (a == 3) THEN GOTO L4;
    EXPORT "TEST", 22;
L4:
    EXPORT "TEST", 23;
ENDPROC;

CRC 0;
