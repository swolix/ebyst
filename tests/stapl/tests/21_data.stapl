NOTE "TEST" "1";
NOTE "TEST" "4";
NOTE "TEST" "5";

ACTION TEST = DO_TEST;

DATA data;
    INTEGER i = 4;
ENDDATA;

PROCEDURE SUB USES data;
    i = i + 1;
ENDPROC;

PROCEDURE TEST USES data, SUB;
    EXPORT "TEST", 1;
    EXPORT "TEST", i;
    CALL SUB;
    EXPORT "TEST", i;
ENDPROC;

PROCEDURE DO_TEST USES TEST;
    CALL TEST;
ENDPROC;

CRC 0;
