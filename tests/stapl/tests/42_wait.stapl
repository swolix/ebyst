NOTE "TEST" "STATE IDLE";
NOTE "TEST" "WAIT 10 CYCLES, 10001 USEC";
NOTE "TEST" "STATE IDLE";
NOTE "TEST" "STATE DRPAUSE";
NOTE "TEST" "WAIT 20 CYCLES, 0 USEC";
NOTE "TEST" "STATE IDLE";
NOTE "TEST" "STATE IDLE";
NOTE "TEST" "WAIT 10 CYCLES, 0 USEC";
NOTE "TEST" "STATE DRPAUSE";
NOTE "TEST" "1";

ACTION TEST = DO_TEST;

PROCEDURE DO_TEST;
    WAIT 10 CYCLES, 10000+1 USEC;
    WAIT DRPAUSE, 2*10 CYCLES;
    WAIT 10 CYCLES, DRPAUSE;
    EXPORT "TEST", 1;
ENDPROC;

CRC 0;
